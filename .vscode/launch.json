{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Broker Server",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/cmd/brokerapi/main.go",
        "cwd": "${workspaceFolder}"
      },
      {
        "name": "Launch Executor WS Server",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${workspaceFolder}/cmd/executorws/main.go",
        "cwd": "${workspaceFolder}"
      },
      {
        "name": "Debug Test",
        "type": "go",
        "request": "launch",
        "mode": "test",
        "program": "${workspaceFolder}/src/test",
        "args": [
          "test",
          "./src/test",
          "-v"
        ]
      }
    ]
  }
  